    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-oidc-context": "^3.1.1",
    "react-router-dom": "^6.28.1",
    "style-loader": "^4.0.0",

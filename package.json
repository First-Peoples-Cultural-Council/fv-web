{
  "name": "fv-web",
  "version": "1.0.0",
  "description": "A React.js front-end for the FirstVoices API.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/First-Peoples-Cultural-Council/fv-web.git"
  },
  "keywords": [
    "indigenous",
    "language",
    "revitalization"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/First-Peoples-Cultural-Council/fv-web/issues"
  },
  "homepage": "https://github.com/First-Peoples-Cultural-Council/fv-web#readme",
  "engines": {
    "node": "^18.16.0"
  },
  "scripts": {
    "start": "webpack serve --env V1_URL=http://0.0.0.0:3001 --config webpack/webpack.development.js",
    "startDev": "webpack serve --env V1_URL=https://dev.firstvoices.com NUXEO_URL=https://dev.firstvoices.com/nuxeo API_URL=https://v2.dev.firstvoices.com/nuxeo/api/v1/ --config webpack/webpack.development.js",
    "startPreprod": "webpack serve --env NUXEO_URL=https://preprod.firstvoices.com API_URL=https://preprod.firstvoices.com/nuxeo/api/v1/ --config webpack/webpack.development.js",
    "startProxiedPreprod": "webpack serve --env NUXEO_URL=https://preprod.firstvoices.com API_URL=https://localhost:3001/nuxeo/api/v1/ --config webpack/webpack.proxied.preprod.js",
    "startInSkaffold": "webpack serve --progress --config webpack/webpack.skaffold.js --disable-host-check",
    "build:production": "webpack --color --progress --config webpack/webpack.production.js",
    "prepare": "husky install",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\""
  },
  "lint-staged": {
    "*.{json,css,md}": "prettier --write",
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@imaginary-cloud/prettier-config",
  "devDependencies": {
    "@imaginary-cloud/eslint-config-react": "^2.0.1",
    "@imaginary-cloud/prettier-config": "^1.0.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.3"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.19.0",
    "@babel/runtime-corejs3": "^7.21.0",
    "@dnd-kit/core": "^6.0.6",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.1",
    "@headlessui/react": "^1.7.4",
    "@hookform/resolvers": "^2.8.10",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@uppy/aws-s3": "^3.0.3",
    "@uppy/core": "^3.0.3",
    "@uppy/dashboard": "^3.1.0",
    "@uppy/drag-drop": "^3.0.1",
    "@uppy/file-input": "^3.0.1",
    "@uppy/image-editor": "^2.0.1",
    "@uppy/progress-bar": "^3.0.1",
    "@uppy/react": "^3.0.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "core-js": "^3.30.1",
    "css-loader": "^6.7.3",
    "cssnano": "^6.0.0",
    "dompurify": "^3.0.2",
    "draft-js": "^0.11.7",
    "howler": "^2.2.3",
    "html-webpack-plugin": "^5.5.1",
    "i18next": "^22.4.15",
    "ky": "^0.33.3",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.2",
    "prop-types": "^15.8.1",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.10.0",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.3.1",
    "webpack": "^5.80.0",
    "webpack-merge": "^5.8.0",
    "yup": "^1.1.1"
  }
}

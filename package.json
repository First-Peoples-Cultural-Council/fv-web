{
  "name": "fv-web",
  "version": "0.24.0",
  "description": "A React.js front-end for the FirstVoices API.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/First-Peoples-Cultural-Council/fv-web.git"
  },
  "keywords": [
    "indigenous",
    "language",
    "revitalization"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/First-Peoples-Cultural-Council/fv-web/issues"
  },
  "homepage": "https://github.com/First-Peoples-Cultural-Council/fv-web#readme",
  "engines": {
    "node": "^22.18.0",
    "npm": "^10.9.3"
  },
  "scripts": {
    "start": "webpack serve --config webpack/webpack.development.js",
    "startDev": "webpack serve --env API_URL=https://api.dev.firstvoices.com/api/1.0/ --config webpack/webpack.development.js",
    "startPreprod": "webpack serve --env APP_ENV=preprod API_URL=https://api.preprod.firstvoices.com/api/1.0/ --config webpack/webpack.development.js",
    "build:production": "webpack --color --progress --config webpack/webpack.production.js",
    "prepare": "husky",
    "lintStaged": "lint-staged",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\""
  },
  "lint-staged": {
    "*.{json,css,md}": "prettier --write",
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.3.1",
    "@eslint/css": "^0.13.0",

{
  "name": "fv-web",
  "version": "0.17.0",
  "description": "A React.js front-end for the FirstVoices API.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/First-Peoples-Cultural-Council/fv-web.git"
  },
  "keywords": [
    "indigenous",
    "language",
    "revitalization"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/First-Peoples-Cultural-Council/fv-web/issues"
  },
  "homepage": "https://github.com/First-Peoples-Cultural-Council/fv-web#readme",
  "engines": {
    "node": "^20.11.1",
    "npm": "^10.2.4"
  },
  "scripts": {
    "start": "webpack serve --env API_URL=http://localhost:8000/api/1.0/ --config webpack/webpack.development.js",
    "startDev": "webpack serve --env APP_ENV=dev BUILD_STRING=0.17.0 API_URL=https://api.dev.firstvoices.com/api/1.0/ --config webpack/webpack.development.js",
    "startPreprod": "webpack serve --env APP_ENV=preprod BUILD_STRING=0.17.0 API_URL=https://api.preprod.firstvoices.com/api/1.0/ --config webpack/webpack.development.js",
    "startInSkaffold": "webpack serve --progress --config webpack/webpack.skaffold.js --disable-host-check",
    "build:production": "webpack --color --progress --config webpack/webpack.production.js",
    "prepare": "husky",
    "lintStaged": "lint-staged",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\""
  },
  "lint-staged": {
    "*.{json,css,md}": "prettier --write",
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.62.16",
    "cypress": "^13.15.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-custom-rules": "file:eslint-rules/data-testid",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testcafe": "^0.2.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.4.3",
    "mochawesome": "^7.1.3",
    "prettier": "^2.8.8",
    "testcafe": "^3.7.2",
    "testcafe-blink-diff": "^0.5.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/eslint-plugin": "^7.25.1",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.26.9",
    "@babel/runtime-corejs3": "^7.24.7",
    "@bcgov/bc-sans": "^2.1.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@headlessui/react": "^2.1.10",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.10.0",
    "@sentry/react": "^8.48.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.62.16",
    "@uppy/core": "^4.4.0",
    "@uppy/dashboard": "^4.3.0",
    "@uppy/drag-drop": "^4.1.0",
    "@uppy/file-input": "^4.1.1",
    "@uppy/image-editor": "^3.3.1",
    "@uppy/progress-bar": "^4.2.1",
    "@uppy/react": "^4.2.1",
    "@uppy/xhr-upload": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "common": "^0.2.5",
    "compression-webpack-plugin": "^11.1.0",
    "core-js": "^3.41.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.5",
    "dompurify": "^3.2.4",
    "draft-js": "^0.11.7",
    "howler": "^2.2.3",
    "html-webpack-plugin": "^5.6.3",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "ky": "^1.7.4",
    "oidc-client-ts": "^3.1.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.5",

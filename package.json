    "@uppy/react": "^3.3.1",
    "@uppy/xhr-upload": "^3.6.6",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "common": "^0.2.5",
    "compression-webpack-plugin": "^11.1.0",
    "core-js": "^3.37.1",
    "css-loader": "^7.1.1",
    "cssnano": "^7.0.1",
    "dompurify": "^3.0.11",
    "draft-js": "^0.11.7",
    "howler": "^2.2.3",
    "html-webpack-plugin": "^5.6.0",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "ky": "^1.3.0",
    "oidc-client-ts": "^3.0.1",
    "postcss": "^8.4.35",
